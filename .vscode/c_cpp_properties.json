{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/"
            ],
            "defines": [
                "KERNEL_NAME=\"Fennix\"",
                "KERNEL_VERSION=\"1.0\"",
                "GIT_COMMIT=\"0000000000000000000000000000000000000000\"",
                "GIT_COMMIT_SHORT=\"0000000\""
            ],
            "compilerPath": "${workspaceFolder}/tools/cross/bin/amd64-elf-gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-fno-rtti",
                "-fexceptions",
                "-fno-pic",
                "-fno-pie",
                "-mno-80387",
                "-mno-mmx",
                "-mno-3dnow",
                "-mno-red-zone",
                "-mno-sse",
                "-mno-sse2",
                "-march=nehalem",
                "-pipe",
                "-mcmodel=kernel",
                "-msoft-float",
                "-fno-builtin"
            ]
        }
    ],
    "version": 4
}